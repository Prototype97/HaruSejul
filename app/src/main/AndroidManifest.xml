<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sierramatcher.harusejul">

    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_mainicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_mainicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WritingActivity" />
        <activity android:name=".SavedActivity" />
        <activity android:name=".RecordActivity"/>
        <receiver android:name=".PushAlarmBroadcastReceiver"/>
    </application>
</manifest>